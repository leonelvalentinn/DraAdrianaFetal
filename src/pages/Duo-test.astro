---
import Layout from '../layouts/Layout.astro';
import NavbarMovil from '../components/NavbarMovil.astro'
import Footer from '../components/Footer.astro';
import Ubicacion from '../components/Ubicacion.astro';
import RedesFlotantes from '../components/RedesFlotantes.astro';
import Servicio from '../components/Servicio.astro';
import Up from '../components/Up.astro';
import Contacto2 from '../components/Contacto2.astro';
import Loader from '../components/Loader.astro';
 
---

<Layout title="Duo Test - Dra. Janeth Adriana García Cid" description='Es un estudio que evalúa la cantidad de dos hormonas placentarias a través de una muestra sanguínea tomada de la madre, que se utilizan en combinación con los marcadores del ultrasonido del primer trimestre para conocer  la probabilidad de enfermedad de los cromosomas con Síndrome de Down, cuando se combinan juntos: marcadores bioquímicos + hallazgos del ultrasonido de primer trimestre la detección será de hasta un 95 % de enfermedad de los cromosomas.  Esta combinación es lo ideal y siempre se recomienda a las pacientes.'>
	<main id="inicio">
    <Loader />
		<NavbarMovil/>
    <Servicio 
      title='¿En que consiste?' 
      punto1='❤ Es un estudio que evalúa la cantidad de dos hormonas placentarias a través de una muestra sanguínea tomada de la madre, que se utilizan en combinación con los marcadores del ultrasonido del primer trimestre para conocer  la probabilidad de enfermedad de los cromosomas con Síndrome de Down, cuando se combinan juntos: marcadores bioquímicos + hallazgos del ultrasonido de primer trimestre la detección será de hasta un 95 % de enfermedad de los cromosomas.  Esta combinación es lo ideal y siempre se recomienda a las pacientes.' 
      punto2=''
      punto3=''
      punto4=''
      punto5=''
      punto6=''
      img='/prueba.webp'
      infoDesplegable1=''
      infoDesplegable2=''
      transition='Duo'
    />
    <Contacto2 />
		<Ubicacion/>
		<RedesFlotantes/>
    <Up />
		<Footer/>
	</main>
</Layout>