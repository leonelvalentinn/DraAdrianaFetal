      punto4=''
      punto5=''
      punto6=''
      img={revelacion1}
      infoDesplegable1=''
      infoDesplegable2=''
      />
      <Testimonios 
        img1={revelacion}
        img2={img2}
        img3={img3}
        img4={img4}
        img5={img5}
      />
    <Contacto2 />
		<Ubicacion/>
		<RedesFlotantes/>
    <Up />
		<Footer/>
	</main>
</Layout>